<div class="container" id="strims_content">
  <div style="display: table; margin: 0 auto;">
    <div id="status"></div>
  </div>
  
  <div id="user_settings" class="row collapse">
    <div class="well well-sm col-md-4 col-md-offset-4">
      <div class="checkbox">
        <label>
          <input type="checkbox" id="show-nsfw"> Show NSFW
        </label>
      </div>
    </div>
  </div> 
 

  <h1 align="center" style="color: #FFFFFF;">
    See what <span id="viewercount"></span> rustlers are watching!
  </h1>
  <div id="strims">
    <%- rendered_streams %>
  </div>
</div>

<script src="/js/bundle.js?cachebuster=<%= new Date() %>"></script>
<script type="text/javascript">
var React = require('react')
console.log('createing react app client side')
  var App = React.createFactory(require("./js/App"));
React.render(App((<%- JSON.stringify(react_props) %>)), document.getElementById("strims"))
</script>
<script type="text/javascript" src="/js/stream_list.js"></script>
